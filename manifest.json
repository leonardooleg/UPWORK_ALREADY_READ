{
  "manifest_version": 3,
  "name": "UPWORK ALREADY READ",
  "description": "Adds a mark where you last read.",
  "version": "1.2.1",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [ "https://www.upwork.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "/icon/get_started16.png",
    "32": "/icon/get_started32.png",
    "48": "/icon/get_started48.png",
    "128": "/icon/get_started128.png"
  }
}