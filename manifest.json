{
  "manifest_version": 2,
  "name": "UPWORK ALREADY READ",
  "description": "Adds a mark where you last read.",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ]
}